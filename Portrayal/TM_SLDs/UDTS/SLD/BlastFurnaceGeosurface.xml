						<se:LabelPlacement>
							<se:LinePlacement>
								<se:PerpendicularOffset>-5</se:PerpendicularOffset>
							</se:LinePlacement>
						</se:LabelPlacement>
						<se:Fill>
							<se:SvgParameter name="fill">#000000</se:SvgParameter>
						</se:Fill>
					</se:TextSymbolizer>
				</se:Rule>

				<!-- AR_Black_BlastFurnace_NotUsable symbol -->
